{% from "influxdb/map.jinja" import influxdb_settings with context -%}

# WARNING: This file is managed by Salt
# DO NOT EDIT MANUALLY

{{ influxdb_settings.logging.directory }}/*.log {
  daily
  rotate 3
  copytruncate
  compress
  delaycompress
  notifempty
  missingok
  su influxdb influxdb
}

